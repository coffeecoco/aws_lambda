{
    "Version": "2012-10-17",
    "Statement": [
    {
        "Effect": "Allow",
        "Action": [
            "kms:Encrypt",
            "kms:Decrypt"
        ],
        "Resource": [
            "arn:aws:kms:*:824366033707:key/*"
        ]
    },
    {
        "Effect": "Allow",
        "Action": [
            "lambda:UpdateFunctionConfiguration",
            "lambda:UpdateFunctionCode"
        ],
        "Resource": [
            "arn:aws:lambda:*:824366033707:*:*"
        ]
    },
    {
        "Effect": "Allow",
        "Action": [
            "iam:PassRole"
        ],
        "Resource": [
            "arn:aws:iam::824366033707:role/devsecopsLambdaEnvVars"
        ]
    }
    ]
}